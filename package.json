{
  "name": "fxjs-modules-orm-mono",
  "mono": {
    "scope": "fxjs",
    "git_path": "fxjs-modules/orm"
  },
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap --no-hoist",
    "bootstrap:noci": "lerna bootstrap --no-hoist --no-ci",
